@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

@prefix pe:   <http://example.org/pe/lesson#> .

#################################################################
# Classes
#################################################################

pe:AchievementStandard a rdfs:Class ;
  rdfs:label "체육 성취기준"@ko .

pe:LessonPlan a rdfs:Class ;
  rdfs:label "체육 지도안"@ko .

pe:GagneEvent a rdfs:Class ;
  rdfs:label "가네의 9가지 수업사태"@ko .

pe:Activity a rdfs:Class ;
  rdfs:label "학습 활동"@ko .

pe:Assessment a rdfs:Class ;
  rdfs:label "평가"@ko .

pe:Material a rdfs:Class ;
  rdfs:label "준비물/자료"@ko .

#################################################################
# Properties
#################################################################

pe:code a rdf:Property ;
  rdfs:label "성취기준 코드"@ko ;
  rdfs:domain pe:AchievementStandard ;
  rdfs:range xsd:string .

pe:description a rdf:Property ;
  rdfs:label "설명"@ko ;
  rdfs:range xsd:string .

pe:hasStandard a rdf:Property ;
  rdfs:label "성취기준 포함"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range pe:AchievementStandard .

pe:hasEvent a rdf:Property ;
  rdfs:label "수업사태 포함"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range pe:GagneEvent .

pe:eventNumber a rdf:Property ;
  rdfs:label "수업사태 번호"@ko ;
  rdfs:domain pe:GagneEvent ;
  rdfs:range xsd:integer .

pe:teacherAction a rdf:Property ;
  rdfs:label "교사 활동"@ko ;
  rdfs:domain pe:GagneEvent ;
  rdfs:range xsd:string .

pe:learnerAction a rdf:Property ;
  rdfs:label "학습자 활동"@ko ;
  rdfs:domain pe:GagneEvent ;
  rdfs:range xsd:string .

pe:timeMinutes a rdf:Property ;
  rdfs:label "소요 시간(분)"@ko ;
  rdfs:domain pe:GagneEvent ;
  rdfs:range xsd:integer .

pe:hasActivity a rdf:Property ;
  rdfs:label "활동 연결"@ko ;
  rdfs:domain pe:GagneEvent ;
  rdfs:range pe:Activity .

pe:hasAssessment a rdf:Property ;
  rdfs:label "평가 연결"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range pe:Assessment .

pe:hasMaterial a rdf:Property ;
  rdfs:label "준비물 포함"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range pe:Material .

pe:targetGrades a rdf:Property ;
  rdfs:label "대상 학년"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range xsd:string .

pe:classDuration a rdf:Property ;
  rdfs:label "수업 시간(분)"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range xsd:integer .

pe:learningGoal a rdf:Property ;
  rdfs:label "학습 목표"@ko ;
  rdfs:domain pe:LessonPlan ;
  rdfs:range xsd:string .

