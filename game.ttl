@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

@prefix pe:   <http://example.org/pe/game#> .

#################################################################
# Classes
#################################################################

pe:Game a rdfs:Class ; rdfs:label "평가 게임"@ko .
pe:Domain a rdfs:Class ; rdfs:label "영역(성취기준 묶음)"@ko .
pe:Mission a rdfs:Class ; rdfs:label "미션(평가 과제)"@ko .
pe:StandardGroup a rdfs:Class ; rdfs:label "성취기준 범위"@ko .
pe:ScoringRule a rdfs:Class ; rdfs:label "채점 규칙"@ko .
pe:AchievementLevel a rdfs:Class ; rdfs:label "성취도 수준"@ko .

#################################################################
# Properties
#################################################################

pe:hasDomain a rdf:Property ;
  rdfs:label "영역 포함"@ko ;
  rdfs:domain pe:Game ;
  rdfs:range pe:Domain .

pe:hasMission a rdf:Property ;
  rdfs:label "미션 포함"@ko ;
  rdfs:domain pe:Domain ;
  rdfs:range pe:Mission .

pe:missionType a rdf:Property ;
  rdfs:label "미션 유형"@ko ;
  rdfs:domain pe:Mission ;
  rdfs:range xsd:string .

pe:prompt a rdf:Property ;
  rdfs:label "미션 안내문"@ko ;
  rdfs:domain pe:Mission ;
  rdfs:range xsd:string .

pe:timeMinutes a rdf:Property ;
  rdfs:label "권장 시간(분)"@ko ;
  rdfs:domain pe:Mission ;
  rdfs:range xsd:integer .

pe:scoreMin a rdf:Property ;
  rdfs:label "최소점수"@ko ;
  rdfs:domain pe:ScoringRule ;
  rdfs:range xsd:integer .

pe:scoreMax a rdf:Property ;
  rdfs:label "최대점수"@ko ;
  rdfs:domain pe:ScoringRule ;
  rdfs:range xsd:integer .

pe:scoringGuide a rdf:Property ;
  rdfs:label "채점 기준"@ko ;
  rdfs:domain pe:ScoringRule ;
  rdfs:range xsd:string .

pe:hasScoring a rdf:Property ;
  rdfs:label "채점 규칙 연결"@ko ;
  rdfs:domain pe:Mission ;
  rdfs:range pe:ScoringRule .

pe:hasStandardGroup a rdf:Property ;
  rdfs:label "성취기준 범위 연결"@ko ;
  rdfs:domain pe:Domain ;
  rdfs:range pe:StandardGroup .

pe:groupLabel a rdf:Property ;
  rdfs:label "범위 이름"@ko ;
  rdfs:domain pe:StandardGroup ;
  rdfs:range xsd:string .

pe:codeStart a rdf:Property ;
  rdfs:label "시작 코드"@ko ;
  rdfs:domain pe:StandardGroup ;
  rdfs:range xsd:string .

pe:codeEnd a rdf:Property ;
  rdfs:label "끝 코드"@ko ;
  rdfs:domain pe:StandardGroup ;
  rdfs:range xsd:string .

pe:alignsTo a rdf:Property ;
  rdfs:label "관련 성취기준 범위"@ko ;
  rdfs:domain pe:Mission ;
  rdfs:range pe:StandardGroup .

pe:domainTotalScore a rdf:Property ;
  rdfs:label "영역 총점"@ko ;
  rdfs:domain pe:Domain ;
  rdfs:range xsd:integer .

pe:levelRule a rdf:Property ;
  rdfs:label "성취도 판정 기준"@ko ;
  rdfs:domain pe:AchievementLevel ;
  rdfs:range xsd:string .

pe:hasAchievementLevel a rdf:Property ;
  rdfs:label "성취도 수준 포함"@ko ;
  rdfs:domain pe:Game ;
  rdfs:range pe:AchievementLevel .

#################################################################
# Game (전체)
#################################################################

pe:PE_Achievement_Game a pe:Game ;
  rdfs:label "중학교 체육 성취기준 평가 게임(미션형)"@ko ;
  pe:hasDomain
    pe:Domain_Health,
    pe:Domain_ActionSports,
    pe:Domain_CombatSports,
    pe:Domain_InvasionSports,
    pe:Domain_FieldSports,
    pe:Domain_NetSports,
    pe:Domain_LifeNatureSports,
    pe:Domain_Expression ;
  pe:hasAchievementLevel pe:Level_High, pe:Level_Mid, pe:Level_Low .

#################################################################
# Achievement Levels (영역당 총점 6점 기준)
#################################################################

pe:Level_High a pe:AchievementLevel ;
  rdfs:label "상"@ko ;
  pe:levelRule "영역 점수 5~6점: 개념 이해 + 실천 정확 + 태도 우수"@ko .

pe:Level_Mid a pe:AchievementLevel ;
  rdfs:label "중"@ko ;
  pe:levelRule "영역 점수 3~4점: 기본 이해/실천 가능, 일부 보완 필요"@ko .

pe:Level_Low a pe:AchievementLevel ;
  rdfs:label "하"@ko ;
  pe:levelRule "영역 점수 0~2점: 이해·실천 모두 어려움, 단계적 지도 필요"@ko .

#################################################################
# Standard Groups (너가 준 8개 범위)
#################################################################

pe:SG_Health a pe:StandardGroup ;
  pe:groupLabel "[9체01-01~9체01-14] 건강"@ko ;
  pe:codeStart "9체01-01" ;
  pe:codeEnd "9체01-14" .

pe:SG_ActionSports a pe:StandardGroup ;
  pe:groupLabel "[9체02-01~9체02-03] 동작형 스포츠"@ko ;
  pe:codeStart "9체02-01" ;
  pe:codeEnd "9체02-03" .

pe:SG_CombatSports a pe:StandardGroup ;
  pe:groupLabel "[9체02-07~9체02-09] 투기형 스포츠"@ko ;
  pe:codeStart "9체02-07" ;
  pe:codeEnd "9체02-09" .

pe:SG_InvasionSports a pe:StandardGroup ;
  pe:groupLabel "[9체02-10~9체02-12] 영역형 스포츠"@ko ;
  pe:codeStart "9체02-10" ;
  pe:codeEnd "9체02-12" .

pe:SG_FieldSports a pe:StandardGroup ;
  pe:groupLabel "[9체02-13~9체02-15] 필드형 스포츠"@ko ;
  pe:codeStart "9체02-13" ;
  pe:codeEnd "9체02-15" .

pe:SG_NetSports a pe:StandardGroup ;
  pe:groupLabel "[9체02-16~9체02-18] 네트형 스포츠"@ko ;
  pe:codeStart "9체02-16" ;
  pe:codeEnd "9체02-18" .

pe:SG_LifeNatureSports a pe:StandardGroup ;
  pe:groupLabel "[9체02-19~9체02-27] 생활/자연환경형 스포츠+태도"@ko ;
  pe:codeStart "9체02-19" ;
  pe:codeEnd "9체02-27" .

pe:SG_Expression a pe:StandardGroup ;
  pe:groupLabel "[9체03-01~9체03-10] 스포츠 표현"@ko ;
  pe:codeStart "9체03-01" ;
  pe:codeEnd "9체03-10" .

#################################################################
# Scoring Rule (공통: 0~2점)
#################################################################

pe:Score_0to2 a pe:ScoringRule ;
  rdfs:label "0~2점 공통 채점"@ko ;
  pe:scoreMin 0 ;
  pe:scoreMax 2 ;
  pe:scoringGuide
    "2점=정확·안전·적극(설명/수행/태도 모두 충족), 1점=부분 성공(기본 수행), 0점=미수행 또는 위험/규칙 위반"@ko .

#################################################################
# Domain 1: 건강(9체01)
#################################################################

pe:Domain_Health a pe:Domain ;
  rdfs:label "영역1 건강 미션"@ko ;
  pe:hasStandardGroup pe:SG_Health ;
  pe:domainTotalScore 6 ;
  pe:hasMission pe:M1_H_Q, pe:M2_H_P, pe:M3_H_R .

pe:M1_H_Q a pe:Mission ;
  rdfs:label "건강 퀴즈 미션"@ko ;
  pe:missionType "Quiz" ;
  pe:prompt "체력 증진/체력 관리/운동 처방의 차이를 1문장씩 말해보기(또는 카드 선택)"@ko ;
  pe:timeMinutes 4 ;
  pe:alignsTo pe:SG_Health ;
  pe:hasScoring pe:Score_0to2 .

pe:M2_H_P a pe:Mission ;
  rdfs:label "건강 실천 미션"@ko ;
  pe:missionType "Practice" ;
  pe:prompt "내 체력 수준에 맞게 3종 순환운동(예: 스쿼트-플랭크-왕복달리기) 강도 선택 후 1세트 수행"@ko ;
  pe:timeMinutes 6 ;
  pe:alignsTo pe:SG_Health ;
  pe:hasScoring pe:Score_0to2 .

pe:M3_H_R a pe:Mission ;
  rdfs:label "건강 성찰 미션"@ko ;
  pe:missionType "Reflection" ;
  pe:prompt "오늘 내 몸 상태를 진단하고(피로/호흡/근육), 다음 시간 목표 1개와 안전 수칙 1개를 적기"@ko ;
  pe:timeMinutes 3 ;
  pe:alignsTo pe:SG_Health ;
  pe:hasScoring pe:Score_0to2 .

